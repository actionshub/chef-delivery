{
  "problemMatcher": [
      {
          "owner": "rubocop",
          "pattern": [
              {
                  "regexp": "^([^\\s].*)\\:(\\d+)\\:(\\d+)\\:\\s+([A-Z])\\:\\s+([^\\\\s].*)\\:\\s+(.*)$",
                  "file": 1,
                  "line": 2,
                  "column": 3,
                  "severity": 4,
                  "code": 5,
                  "message": 6
              }
          ]
      }
  ]
}
